<rdf:RDF
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
    xmlns:pmlp="http://inference-web.org/2.0/pml-provenance.owl#"
    xmlns:viskoO="http://trust.utep.edu/visko/ontology/visko-operator.owl#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#">
  <owl:Ontology rdf:about="https://raw.github.com/nicholasdelrio/visko/master/rdf/plain-text-viewer.owl">
    <owl:imports rdf:resource="http://inference-web.org/2.0/pml-provenance.owl"/>
    <owl:imports rdf:resource="http://trust.utep.edu/visko/ontology/visko-operator.owl"/>
  </owl:Ontology>
  <viskoO:Viewer rdf:about="https://raw.github.com/nicholasdelrio/visko/master/rdf/plain-text-viewer.owl#plain-text-viewer">
    <rdfs:comment>Does nothing really, just extracts the text from the PML conclusion...</rdfs:comment>
    <viskoO:partOfViewerSet rdf:resource="https://raw.github.com/nicholasdelrio/visko/master/rdf/probeit.owl#probeit"/>
    <rdfs:label>Plain Text Viewer</rdfs:label>
    <viskoO:operatesOn rdf:resource="https://raw.github.com/nicholasdelrio/visko/master/rdf/formats/PLAIN.owl#PLAIN"/>
    <viskoO:operatesOn rdf:resource="https://raw.github.com/nicholasdelrio/visko/master/rdf/formats/PLAINTEXT.owl#PLAINTEXT"/>
    <viskoO:partOfViewerSet rdf:resource="https://raw.github.com/nicholasdelrio/visko/master/rdf/internet-explorer.owl#internet-explorer"/>
    <viskoO:operatesOn rdf:resource="https://raw.github.com/nicholasdelrio/visko/master/rdf/formats/VNDLATEXZ.owl#VNDLATEXZ"/>
    <pmlp:hasName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >plain-text-viewer</pmlp:hasName>
    <viskoO:partOfViewerSet rdf:resource="https://raw.github.com/nicholasdelrio/visko/master/rdf/mozilla-firefox.owl#mozilla-firefox"/>
  </viskoO:Viewer>
</rdf:RDF>
